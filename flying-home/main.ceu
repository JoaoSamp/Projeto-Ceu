#include "c.ceu"
#include "sdl/sdl.ceu"
#include "character.ceu"

var SDL_Color bg = val SDL_Color(0x00,0x00,0x00,0xFF);


var&? SDL_Init sdl = spawn SDL_Init("Flying Home - Basic Character", 640,480, bg);
watching sdl do
    spawn Character (100, 100, 64, 64, &sdl.ren);
    await FOREVER;
end

escape 0;