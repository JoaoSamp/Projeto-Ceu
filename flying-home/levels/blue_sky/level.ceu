#include "map.ceu"
#include "cloud.ceu"
#include "rings.ceu"

code/await Blue_Sky ( var&? Player plr, var& _SDL_Renderer ren ) -> NEVER
do
    var Vector map_position = val Vector (0, 0);
	var& Map map = spawn Map (map_position, &ren, &plr!);
	spawn Cloud_Manager(&map, 1, &ren);
	spawn Ring_Manager(&map, 2, &ren);
	await FOREVER;
end